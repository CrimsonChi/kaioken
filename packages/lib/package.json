{
  "name": "kaioken",
  "version": "0.28.0",
  "description": "A powerful, easy-to-use rendering library with a tiny footprint",
  "main": "index.js",
  "type": "module",
  "homepage": "https://kaioken.dev",
  "exports": {
    ".": {
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "import": {
        "types": "./index.d.ts",
        "default": "./index.js"
      }
    },
    "./utils": {
      "require": {
        "types": "./utils.d.ts",
        "default": "./utils.js"
      },
      "import": {
        "types": "./utils.d.ts",
        "default": "./utils.js"
      }
    },
    "./ssr/client": {
      "require": {
        "types": "./ssr/client.d.ts",
        "default": "./ssr/client.js"
      },
      "import": {
        "types": "./ssr/client.d.ts",
        "default": "./ssr/client.js"
      }
    },
    "./ssr/server": {
      "require": {
        "types": "./ssr/server.d.ts",
        "default": "./ssr/server.js"
      },
      "import": {
        "types": "./ssr/server.d.ts",
        "default": "./ssr/server.js"
      }
    },
    "./jsx-dev-runtime": {
      "require": {
        "types": "./jsx-dev-runtime.d.ts",
        "default": "./jsx-dev-runtime.js"
      },
      "import": {
        "types": "./jsx-dev-runtime.d.ts",
        "default": "./jsx-dev-runtime.js"
      }
    },
    "./jsx-runtime": {
      "require": {
        "types": "./jsx-runtime.d.ts",
        "default": "./jsx-runtime.js"
      },
      "import": {
        "types": "./jsx-runtime.d.ts",
        "default": "./jsx-runtime.js"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CrimsonChi/kaioken"
  },
  "scripts": {
    "build": "rm -rf dist && pnpm i && tsc",
    "test": "node --test",
    "dev": "tsc --watch"
  },
  "types": "index.d.ts",
  "keywords": [
    "kaioken",
    "kaiokenjs",
    "kaioken.js",
    "react"
  ],
  "author": "LankyMoose",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.3.16",
    "@types/jsdom": "^21.1.7",
    "@types/mocha": "^10.0.7",
    "chai": "^5.1.1",
    "mocha": "^10.7.0",
    "ts-mocha": "^10.0.0",
    "typescript": "^5.5.3"
  }
}
