{
  "name": "kaioken",
  "version": "0.38.1",
  "description": "A powerful, easy-to-use rendering library with a tiny footprint",
  "main": "dist/index.js",
  "type": "module",
  "homepage": "https://kaioken.dev",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./form": {
      "types": "./dist/form/index.d.ts",
      "default": "./dist/form/index.js"
    },
    "./router": {
      "types": "./dist/router/index.d.ts",
      "default": "./dist/router/index.js"
    },
    "./swr": {
      "types": "./dist/swr.d.ts",
      "default": "./dist/swr.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "default": "./dist/utils.js"
    },
    "./ssr/client": {
      "types": "./dist/ssr/client.d.ts",
      "default": "./dist/ssr/client.js"
    },
    "./ssr/server": {
      "types": "./dist/ssr/server.d.ts",
      "default": "./dist/ssr/server.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx.d.ts",
      "default": "./dist/jsx.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx.d.ts",
      "default": "./dist/jsx.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CrimsonChi/kaioken"
  },
  "scripts": {
    "build": "rm -rf dist && pnpm i && tsc",
    "test": "node --test",
    "dev": "tsc --watch"
  },
  "types": "index.d.ts",
  "keywords": [
    "kaioken",
    "kaiokenjs",
    "kaioken.js",
    "react"
  ],
  "author": "LankyMoose",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^5.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/mocha": "^10.0.10",
    "chai": "^5.1.2",
    "ts-mocha": "^10.0.0",
    "typescript": "^5.7.2"
  }
}
